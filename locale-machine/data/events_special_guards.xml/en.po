"data/events_special_guards.xml$//event[@name=\"REINFORCEMENTS_ORCHID\"]/beaconType/unvisitedTooltip"
msgstr "An unvisited location."

#: src-en/data/events_special_guards.xml:2398
msgid ""
"data/events_special_guards.xml$//event[@name=\"REINFORCEMENTS_ORCHID\"]/beaconType/visitedTooltip"
msgstr "Explored location. Nothing left of interest."

#: src-en/data/events_special_guards.xml:2399
msgid ""
"data/events_special_guards.xml$//event[@name=\"REINFORCEMENTS_ORCHID\"]/beaconType/undiscoveredTooltip"
