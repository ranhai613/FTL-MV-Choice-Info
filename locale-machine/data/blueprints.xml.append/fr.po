#: src-en/data/blueprints.xml.append:32453
msgid "data/blueprints.xml.append$//weaponBlueprint[@name=\"MODULE_BIO\"]/title"
msgstr "Module Anti-Biologique"

#: src-en/data/blueprints.xml.append:32454
