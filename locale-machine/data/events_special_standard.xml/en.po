#: src-en/data/events_special_standard.xml:6373
msgid ""
"data/events_special_standard.xml$//event[@name=\"STOREEVENT_REBEL_ENGINEER\"]/choice[2]/event/text"
msgstr "\"Well if you aren't going to be helpful, then we'll just blow you up!\""

#: src-en/data/events_special_standard.xml:6376
msgid ""
"data/events_special_standard.xml$//event[@name=\"STOREEVENT_REBEL_ENGINEER\"]/choice[2]/event/choice/text"
msgstr "Prepare to fight!"

