#: src-en/data/events_special_standard.xml:6234
msgid ""
"data/events_special_standard.xml$//event[@name=\"STOREEVENT_REBEL_XENOPHOBE\"]/choice[@req=\"LIST_CREW_PETS\"]/text"
msgstr "(Non-Sapient Crew) Pass off your alien crew as pets."

#: src-en/data/events_special_standard.xml:6236
msgid ""
"data/events_special_standard.xml$//event[@name=\"STOREEVENT_REBEL_XENOPHOBE\"]/choice[@req=\"LIST_CREW_PETS\"]/event/text"
msgstr ""
"The store-owners consider for a bit, but seem to accept this, and allow you "
