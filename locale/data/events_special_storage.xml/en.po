msgid ""
"data/events_special_storage.xml$//event[@name=\"ANCIENT_LORE_DUMP\"]/choice/event/choice/event/choice[@req=\"unique_wither\"]/text"
msgstr "(Aenwithe) \"Hello, me.\""

#: src-en/data/events_special_storage.xml:21836
msgid ""
"data/events_special_storage.xml$//event[@name=\"ANCIENT_LORE_DUMP\"]/choice/event/choice/event/choice[@req=\"unique_wither\"]/event/text"
msgstr ""
"\"Ah, I see you have already encountered me from another timeline. Very well then, I'm sure you understand the situation. Shall we head to the Obelisk sector now, or do you have other things to do?\"\n"
"\n"
"[NOTE: Wither will not occupy a crew slot and therefore doesn't require dismissal of a crew member, despite what the probable disclaimer below may claim.]"

#: src-en/data/events_special_storage.xml:21840
msgid ""
"data/events_special_storage.xml$//event[@name=\"ANCIENT_LORE_DUMP\"]/choice/event/choice/event/choice[@req=\"unique_wither\"]/event/choice[1]/text"
msgstr "Let's go."

#: src-en/data/events_special_storage.xml:21842
msgid ""
"data/events_special_storage.xml$//event[@name=\"ANCIENT_LORE_DUMP\"]/choice/event/choice/event/choice[@req=\"unique_wither\"]/event/choice[1]/event/text"
msgstr ""
